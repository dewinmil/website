<!DOCTYPE html>
<html>
<head>
  <style>
    h1{font-size: 48px; text-transform: uppercase; letter-spacing: 2px; text-align: center;}
    h2{font-size: 24px; text-transform: uppercase; letter-spacing: 2px; text-align: center; color: white;}
    p, li{font-size: 16px; text-align: center; color: white;}
  </style>
  <link href="css/style.css" rel="stylesheet" type ="text/css">
</head>
<body>
  <header>
    <div class="row">
      <% include partial/nav.ejs %>
    </div>
    <h1>Welcome to the profile of Miles DeWind</h1>
    <h2>Age:<%= data.age %></h2>
    <h2>Hobbies</h2>
    <% data.hobbies.forEach(function(item){ %>
      <li><%= item %></li>
    <% }); %>
  </header>

</body>
</html>
